         spre_grupo_material.descripcion  
    FROM spre_grupo_material  
   WHERE ( spre_grupo_material.cvedepto = :p_depto ) AND  
         ( spre_grupo_material.cvearea = :p_area ) AND  
         ( spre_grupo_material.activo = 1 )   AND
		(spre_grupo_material.activo=1 OR spre_grupo_material.activo is null)
