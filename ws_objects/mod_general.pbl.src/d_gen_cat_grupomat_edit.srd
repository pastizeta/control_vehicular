 column=(type=long update=yes updatewhereclause=yes name=activo dbname="spre_grupo_material.activo" dbalias=".activo" )
 retrieve="  SELECT spre_grupo_material.cvegrupo,   
         spre_grupo_material.descripcion,   
         spre_grupo_material.cvedepto,   
         spre_grupo_material.cvearea,
		(select count(cvedepto)
		from spre_materiales
		where 
		spre_materiales.cvedepto = spre_grupo_material.cvedepto AND
		spre_materiales.cvearea = spre_grupo_material.cvearea AND
		spre_materiales.cvegrupo = spre_grupo_material.cvegrupo AND
		spre_materiales.activo=1) as borrable,
		spre_grupo_material.activo  
    FROM spre_grupo_material  
   WHERE ( spre_grupo_material.cvedepto = :p_depto ) AND  
         ( spre_grupo_material.cvearea = :p_area ) AND  
         (spre_grupo_material.activo=1 OR spre_grupo_material.activo is null)
ORDER BY spre_grupo_material.descripcion ASC   
" update="spre_grupo_material" updatewhere=0 updatekeyinplace=yes arguments=(("p_depto", number),("p_area", number)) )
