table(column=(type=long updatewhereclause=yes name=id_inventario dbname="id_inventario" )
 column=(type=long updatewhereclause=yes name=id_cria dbname="id_cria" )
 column=(type=char(50) updatewhereclause=yes name=nombre dbname="nombre" )
 column=(type=char(100) updatewhereclause=yes name=prov_descripcion dbname="prov_descripcion" )
 column=(type=char(6) updatewhereclause=yes name=sexo dbname="sexo" )
 retrieve="select id_inventario, id_cria, nombre, prov_descripcion, sexo
from(
    select a.id_inventario, b.id_cria, b.nombre, b.prov_descripcion, isnull(c.folio,0) as asignado,
    CASE b.sexo
          WHEN 'M' THEN 'Macho'
        WHEN 'H' THEN 'Hembra'
    END as sexo
    from spre_animales_inventario a
    join spre_animales_catalogo b on a.id_cria = b.id_cria
    left join spre_animales_asigna_det c on a.id_inventario = c.id_inventario and c.estatus = 1
    where a.estatus = 1
) as a
where asignado = 0
and (id_inventario = :p_inventario or :p_inventario = 0)
and (upper(nombre) like :p_nombre or :p_nombre = '%%')
order by nombre" arguments=(("p_nombre", string),("p_inventario", number)) )
