event clicked;	string nombre_archivo,nombre
	integer value
if isvalid(dw) then
	SetPointer(HourGlass!)
	value = GetFileOpenName("Selecciona el Archivo a Importar", &
				+ nombre_archivo, nombre, "DOC", &
	+ "Archivos de Texto (*.TXT),*.TXT," &
	+ "Todos Los Archivos (*.*),*.*")
IF value = 1 THEN 	
	if dw.importfile(nombre_archivo)<1 then
		messagebox("Mensaje del Sistema","No se pudo importar la información",stopsign!)
	end if
end if
end if
end event

