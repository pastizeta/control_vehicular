event clicked;string		ls_cvemodulo, ls_wintype

if gi_depto=8 and gi_area=3 then
	ls_wintype='w_dis_prestamo_material'
	opensheet (w_dis_prestamo_material,ls_wintype,w_ppal,0,original!)
elseif gi_depto=12 then
	ls_wintype='w_fis_prestamo_material'
	opensheet (w_fis_prestamo_material,ls_wintype,w_ppal,0,original!)
else
	if gi_depto=10 then
		ls_wintype='w_imd_control_prestamo'
		opensheet (w_imd_control_prestamo,ls_wintype,w_ppal,0,original!)
	end if
	if gi_depto=11 then
		ls_wintype='w_ccd_prestamo_material'
		opensheet (w_ccd_prestamo_material,ls_wintype,w_ppal,0,original!)
	end if
	if gi_depto=12 then
		ls_wintype='w_fis_prestamo_material'
		opensheet (w_fis_prestamo_material,ls_wintype,w_ppal,0,original!)
	end if
	if gi_depto=13 then
		ls_wintype='w_dit_prestamo_material'
		opensheet (w_dit_prestamo_material,ls_wintype,w_ppal,0,original!)
	else
		if gi_depto=8 and gi_area=1 then
			ls_wintype='w_cor_control_prestamo'
			opensheet (w_cor_control_prestamo,ls_wintype,w_ppal,0,original!)
		else
			ls_cvemodulo	=	 is_cvemodulo[gi_depto]
